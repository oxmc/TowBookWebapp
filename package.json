{
  "name": "DisWeb",
  "version": "2.0.5",
  "description": "A Discord Desktop app for linux, built with electron-builder.",
  "main": "src/js/main.js",
  "scripts": {
    "start": "electron .",
    "build-armv7l": "electron-builder --linux appimage --armv7l",
    "build-arm64": "electron-builder --linux appimage --arm64"
  },
  "build": {
    "linux": {
      "target": "appimage",
      "icon": "build/app.png",
      "category": "Network"
    }
  },
  "keywords": [
    "Discord",
    "linux",
    "electron",
    "webapp"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oxmc/disweb.git"
  },
  "author": {
    "name": "oxmc",
    "email": " oxmc7769@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "electron": "^13.1.9",
    "electron-builder": "^22.11.7"
  },
  "dependencies": {
    "decompress": "^4.2.1",
    "fs": "^0.0.1-security",
    "node-notifier": "^10.0.0",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "request": "^2.88.2"
  }
}
